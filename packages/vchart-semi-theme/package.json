    "@internal/ts-config": "workspace:*",
    "@internal/jest-config": "workspace:*",
    "@internal/chart-demo": "workspace:*",
    "jest": "~29.5.0",
    "@jest/globals": "~29.5.0",
    "ts-jest": "~29.1.0",
    "@types/jest": "~29.5.0",
