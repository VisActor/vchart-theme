{
  "name": "@visactor/vchart-semi-theme",
  "version": "1.11.5",
  "description": "Extended themes for VChart",
  "sideEffects": false,
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "files": [
    "cjs",
    "esm",
    "build",
    "public"
  ],
  "scripts": {
